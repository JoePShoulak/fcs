<% provide(:title, "Music") %>
<header class="center border">
	<h1>Preston Gunderson</h1>
</header>
<section class="center border main">
	<p class="indent">Click on an album to view it in iTunes</p>
	<% @albums.each do |album| %>
		<section class='album'>
			<%= link_to( image_tag(album[1]), album[0] ) %>
			<p> <%= album[2] %> </p>
		</section>
	<% end %>
	<a>Getting Good at Starting Over</a> (Coming soon!)
</section>