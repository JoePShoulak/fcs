<% provide(:title, "PHOTOS") %>
<section class="center border main">
	<h3>PHOTOS</h3>
	<p class="indent">Click to view a larger version.</p>
	<% @photos.each do |photo| %>
	<section class="gallery">
		<section class="photo">
			<%= link_to image_tag(photo[0], :height => 200), photo[1], class: "fancybox" %>
		</section>
	</section>
	<% end %>
</section>